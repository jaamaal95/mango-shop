<div class="work-header">
    <span class="work-title"><%= data.title %></span>
    <span class="work-company">at <%= data.company %></span>
    <span class="project-duration"><%= data.duration %></span>
</div>
<% if (data.link) { %>
<div class="work-link">
    <a href="<%= data.link %>" target="_blank"><%= data.link %></a>
</div>
<% } %>
<div class="work-desc work">
    <ul class="work-desc-list">
    <% data.desc.forEach((line) => { %>
        <li class="work-desc-line">
            <span><%- line %></span>
        </li>
    <% }); %>
    </ul>
</div>
